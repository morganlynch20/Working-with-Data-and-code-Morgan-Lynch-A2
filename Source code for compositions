playNote(0)
playNote(6)
playNote(0)
playNote(6)
playNote(0)


#Different notes with duration of half a beat, rest 1 beat inbetween
playNote(35, beats=0.5)
playNote(35, beats=0.5)
rest(1)
playNote(35, beats=0.5)
playNote(35, beats=0.5)
rest(1)
playNote(35, beats=0.5)
playNote(35, beats=0.5)


#looping of code 4 times 
for i in range(4):
    playNote(0)
    fastForward(0.5)
    playNote(0, beats = 0.5, velocity = 40)


#first code( loop four times) notes 1 and 11, rewind for half a beat, note 0 for half a beat with soft velocity) 
for i in range(4):
    playNote([1, 1])
    rewind(0.5)
    playNote(0, beats = 0.5, velocity = 40)

moveTo(0)
for i in range(2):
    rest(1)
    playNote(2)

moveTo(0)
for i in range(4): 
    playNote(4, beats = 0.5, velocity = 50)
    rest(0.5)


#notes 10 and 5, with half a beat and relatively low velocity of 40 
playNote(10, beats = 0.5)
playNote(5, beats=0.5, velocity=40)
playNote(10,beats=0.5)
playNote(5, beats=0.5,velocity=40)
playNote(10, beats=0.5)
playNote(5, beats=0.5,velocity=40)
playNote(10, beats=0.5)
playNote(5, beats=0.5, velocity=40)



# Progression of Dminor, Aminor7,Dminor,Gsus24 and A minor 7 again, played for duration of half a beat with rests of half a beat or 0.25 creating dynamic rhythm
Dmin = [ 50, 53, 57 ]
Amin7 = [ 55, 57, 60, 64 ]
Gsus24 = [ 55, 57, 60, 62 ]

# play two D minor chords
playNote(Dmin, beats = 0.5)
rest(0.5)
playNote(Dmin, beats = 0.5)
rest(0.25)

playNote(Amin7, beats = 0.25)
rest(0.5)
playNote(Amin7, beats = 0.25)
rest(0.25)

playNote(Dmin, beats = 0.5)
rest(0.5)
playNote(Dmin, beats = 0.5)
rest(0.25)

playNote(Gsus24, beats = 0.5)
rest(0.25)
playNote(Gsus24, beats = 0.5)
rest(0.25)

playNote(Amin7, beats = 0.25)
rest(0.5)
playNote(Amin7, beats = 0.25)
rest(0.25)



